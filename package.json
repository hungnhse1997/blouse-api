    "jest": "^26.3.0",
    "jest-html-reporters": "^2.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.5"
  },
  "jest": {
    "roots": [
      "<rootDir>/test/test-blouse-server"
    ],
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "reporters": [
      "default",
      [
        "jest-html-reporters",
        {
          "publicPath": "./coverage",
          "filename": "index.html",
          "expand": true
        }
      ]
    ]
  }
