    "@types/multer": "^1.4.4",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.4",
    "@types/passport-google-oauth20": "^2.0.3",
    "@types/uuid": "^8.0.0",
    "body-parser": "^1.19.0",
    "class-validator": "^0.12.2",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
